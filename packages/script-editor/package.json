{
  "name": "@elyra/script-editor",
  "version": "4.0.0-dev",
  "description": "JupyterLab - Run python and R scripts using a kernel runtime",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "homepage": "https://github.com/elyra-ai/elyra",
  "bugs": {
    "url": "https://github.com/elyra-ai/elyra/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elyra-ai/elyra/"
  },
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "src/**/*.{ts,tsx}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
    "build:prod": "jlpm clean && jlpm build:lib:prod && jlpm build:labextension",
    "build:test": "tsc --build tsconfig.test.json",
    "clean": "jlpm clean:lib",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "clean:labextension": "rimraf elyra_script_editor/labextension elyra_script_editor/_version.py",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "dist": "npm pack .",
    "install:extension": "jlpm build",
    "lab:dev": "jupyter labextension develop --overwrite elyra_script_editor/labextension",
    "lab:install": "jupyter labextension install --no-build",
    "lab:uninstall": "jupyter labextension uninstall --no-build",
    "link:dev": "yarn link @jupyterlab/builder",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "test": "jest --coverage",
    "unlink:dev": "yarn unlink @jupyterlab/builder",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w --sourceMap"
  },
  "dependencies": {
    "@jupyterlab/apputils": "^4.1.6",
    "@jupyterlab/codeeditor": "^4.0.6",
    "@jupyterlab/docregistry": "^4.0.6",
    "@jupyterlab/fileeditor": "^4.0.6",
    "@jupyterlab/logconsole": "^4.0.6",
    "@jupyterlab/outputarea": "^4.0.6",
    "@jupyterlab/rendermime": "^4.0.6",
    "@jupyterlab/services": "^7.0.6",
    "@jupyterlab/ui-components": "^4.0.6",
    "@lumino/signaling": "^2.1.2",
    "@lumino/widgets": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.0.9"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.6",
    "@jupyterlab/testutils": "^4.0.6",
    "@types/jest": "^23.3.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "jest": "^24.7.1",
    "jest-raw-loader": "^1.0.1",
    "rimraf": "~5.0.5",
    "ts-jest": "^24.0.2",
    "typescript": "~5.1.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": false,
    "outputDir": "elyra_script_editor/labextension"
  }
}
